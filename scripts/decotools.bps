def pc(){
    align(xz){
        /summon armor_stand ~0.5 ~ ~0.5 {Tags:["trg","deco","dc_pc"],NoGravity:1,Invisible:1}
        with(@e[tag=trg]){
            armor_stand.setmodel(diamond_hoe,9)
            float x
            x = random.range(-90,180)
            @s.rot_x = x
            untag(trg)
        }
    }
}