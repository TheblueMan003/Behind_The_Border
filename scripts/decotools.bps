package decotools

def lamp(){
    /summon armor_stand ~ ~ ~ {Tags:["trg","deco","dc_lamp"],NoGravity:1,Invisible:1}
    with(@e[tag=trg]){
        armor_stand.setmodel(diamond_hoe,2)
        untag(trg)
    }
}
